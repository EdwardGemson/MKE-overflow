Vote Total:<%=@question.total_votes%>

<% if logged_in? && current_user != @question.user && current_user.has_voted?(@question) == false%>
  <%= button_to 'Upvote', upvote_question_path(@question), method: :post%>
  <%= button_to 'Downvote', downvote_question_path(@question), method: :post%>
<%end%>

